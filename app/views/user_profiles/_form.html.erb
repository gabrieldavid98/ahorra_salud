<%= form_with(model: user_profile) do |form| %>
  <% if user_profile.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(user_profile.errors.count, "error") %> prohibited this user_profile from being saved:</h2>

      <ul>
        <% user_profile.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :user_id, style: "display: block" %>
    <%= form.text_field :user_id %>
  </div>

  <div>
    <%= form.label :names, style: "display: block" %>
    <%= form.text_field :names %>
  </div>

  <div>
    <%= form.label :last_names, style: "display: block" %>
    <%= form.text_field :last_names %>
  </div>

  <div>
    <%= form.label :identification_type_id, style: "display: block" %>
    <%= form.text_field :identification_type_id %>
  </div>

  <div>
    <%= form.label :identification, style: "display: block" %>
    <%= form.number_field :identification %>
  </div>

  <div>
    <%= form.label :sex, style: "display: block" %>
    <%= form.check_box :sex %>
  </div>

  <div>
    <%= form.label :birthdate, style: "display: block" %>
    <%= form.date_field :birthdate %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
